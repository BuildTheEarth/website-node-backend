{
  "name": "website-node-backend",
  "version": "1.0.576",
  "description": "The backend for the BuildTheEarth.net website",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node --import ./src/util/SentryInstrument.js index.js",
    "start-nodemon": "nodemon index.js",
    "start:migrate": "npx prisma migrate deploy && node index.js",
    "prebuild": "node -p \"'export const LIB_VERSION = ' + JSON.stringify(require('./package.json').version) + ';'+'export const LIB_LICENSE = ' + JSON.stringify(require('./package.json').license) + ';'\" > src/util/package.ts",
    "build": "yarn install --pure-lockfile && tsc && yarn sentry:sourcemaps",
    "studio": "prisma studio",
    "prettier": "prettier --write ./src",
    "seed": "esbuild prisma/seed.ts --outfile=node_modules/tmp-seed.cjs --platform=node --bundle --format=cjs --external:prisma --external:@prisma/client && node node_modules/tmp-seed.cjs --preview-feature",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org buildtheearth --project main-backend ./src && sentry-cli --url https://sentry.buildtheearth.net sourcemaps upload --org buildtheearth --project main-backend ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BuildTheEarth/website-node-backend.git"
  },
  "author": "BuildTheEarth Developers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BuildTheEarth/website-node-backend/issues"
  },
  "homepage": "https://api.buildtheearth.net",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/multer": "^1.4.8",
    "@types/node": "^18.11.14",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.29.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "postcss": "^8.4.31",
    "prisma": "5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.427.0",
    "@bte-germany/terraconvert": "^1.0.1",
    "@keycloak/keycloak-admin-client": "^20.0.2",
    "@prisma/client": "5",
    "@sentry/cli": "^2.33.1",
    "@sentry/node": "^8.26.0",
    "@turf/helpers": "^6.5.0",
    "@turf/turf": "^6.5.0",
    "axios": "^1.7.4",
    "blurhash": "^2.0.5",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "dotenv": "^16.0.3",
    "express": "^4.19.2",
    "express-prom-bundle": "^6.6.0",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "express-yup-middleware": "^1.2.3",
    "jsonwebtoken": "^9.0.0",
    "keycloak-connect": "^22.0.0",
    "mariadb": "^3.0.2",
    "minimatch": "^7.4.3",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "plaiceholder": "^3.0.0",
    "prom-client": "^15.0.0",
    "reflect-metadata": "^0.1.13",
    "rfdc": "^1.3.0",
    "sharp": "^0.32.6",
    "uuid": "^9.0.1",
    "winston": "^3.8.1",
    "yup": "^1.0.0"
  }
}